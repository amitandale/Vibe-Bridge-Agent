{
  "name": "bridge-agent",
  "private": true,
  "type": "module",
  "scripts": {
    "test": "NODE_OPTIONS=\"--require ./tests/harness/preload.cjs\" node --test tests/all.test.mjs",
    "test:ci": "c8 -r text -r lcov node --test tests/all.test.mjs",
    "coverage": "c8 report --reporter=text",
    "capacity:dry-run": "node scripts/workspace/dry-run.ci.mjs",
    "check": "bash -lc \"find . -type f \\( -name '*.mjs' -o -name '*.js' \\) -print0 | xargs -0 -r node --check\"",
    "pretest": "npm run check",
    "start": "next start -p ${PORT:-3002} -H ${ORCH_BIND:-127.0.0.1}",
    "build": "next build",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "lint": "eslint . --max-warnings=0"
  },
  "dependencies": {
    "next": "14.2.6",
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "devDependencies": {
    "typescript": "^5.6.3",
    "@types/node": "^20.11.30",
    "@types/react": "^18.2.66"
  }
}
